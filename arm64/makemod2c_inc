./model_ion_channels.c: ../model_ion_channels.mod
	@printf " -> $(C_GREEN)NMODL$(C_RESET) $<\\n"
	(cd "..";  MODLUNIT=$(NRNUNITS) $(NOCMODL) "model_ion_channels.mod" -o "/Users/vartanarakelian/Documents/GitHub/MS_ReflexCircuit/arm64")

./model_ion_channels.o: ./model_ion_channels.c
	@printf " -> $(C_GREEN)Compiling$(C_RESET) $<\\n"
	$(COMPILE) -I".." $(INCLUDES) -fPIC -c $< -o $@

