./km.c: /Users/vartanarakelian/Documents/GitHub/MS_ReflexCircuit/km.mod
	@printf " -> $(C_GREEN)NMODL$(C_RESET) $<\\n"
	(cd "/Users/vartanarakelian/Documents/GitHub/MS_ReflexCircuit";  MODLUNIT=$(NRNUNITS) $(NOCMODL) "km.mod" -o "/Users/vartanarakelian/Documents/GitHub/MS_ReflexCircuit/arm64")

./km.o: ./km.c
	@printf " -> $(C_GREEN)Compiling$(C_RESET) $<\\n"
	$(COMPILE) -I"/Users/vartanarakelian/Documents/GitHub/MS_ReflexCircuit" $(INCLUDES) -fPIC -c $< -o $@

